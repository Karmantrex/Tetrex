function sortByTeamAZ() {
  const sheet = SpreadsheetApp.getActiveSheet();
  const lastRow = sheet.getLastRow();
  const lastCol = sheet.getLastColumn();
  if (lastRow < 2) return; // only header present, nothing to sort

  // Sort all data rows (2..lastRow) across all columns (A..lastCol) by column F
  const dataRange = sheet.getRange(2, 1, lastRow - 1, lastCol);
  dataRange.sort([{ column: 6, ascending: true }]);  // column F = 6
}
